{
    "install_additional_node": {
        "image": "debian:11",
        "features": {
            "node-nvm-use": {
                "version": "lts",
                "additionalVersions": "v17.9.1,v14.19.3"
            }
        }
    },
    "non_root_user": {
        "image": "mcr.microsoft.com/devcontainers/base",
        "remoteUser": "vscode",
        "features": {
            "node-nvm-use": {
                "version": "latest"
            }
        }
    },
    "version_none": {
        "image": "mcr.microsoft.com/devcontainers/base",
        "remoteUser": "vscode",
        "features": {
            "node-nvm-use": {
                "version": "none"
            }
        }
    },
    "install_node_on_universal_image": {
        "image": "mcr.microsoft.com/devcontainers/universal",
        "remoteUser": "codespace",
        "features": {
            "node-nvm-use": {
                "version": "19.1.0"
            }
        }
    },
    "install_node_16_on_bionic": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu-18.04",
        "features": {
            "node-nvm-use": {
                "version": "16"
            }
        }
    }
}
